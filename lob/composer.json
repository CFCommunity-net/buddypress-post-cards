{
    "name": "lob/lob-php",
    "type": "library",
    "description": "Lob.com API PHP wrapper.",
    "keywords": ["lob", "printing", "api", "rest", "web service", "wrapper"],
    "homepage": "https://www.lob.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Lob",
            "email": "support@lob.com",
            "homepage": "https://www.lob.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzle/guzzle": "~3.7"
    },
    "autoload": {
        "psr-0": {
            "Lob\\Tests": "tests/",
            "Lob": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "satooshi/php-coveralls": "dev-master",
        "dompdf/dompdf": "0.6.*"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
